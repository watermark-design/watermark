{
  "name": "watermark-design",
  "version": "1.x",
  "private": true,
  "description": "watermark-design monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky install",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:prettier": "prettier --write \"{docs,packages}/**/*.{js,jsx,cjs,json,tsx,ts,css,less,scss,vue,html,md}\"",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{docs,packages}/**/*.{js,jsx,cjs,tsx,ts,vue}\" --fix",
    "lint:stylelint": "stylelint --cache --fix \"{docs,packages}/**/*.{css,less,scss,styl,postcss,vue,html}\" --cache --cache-location node_modules/.cache/stylelint/",
    "dev": "pnpm -r --aggregate-output --filter=./playground dev",
    "dev2.6": "pnpm -r --aggregate-output --filter=./playground-vue2.6 dev",
    "dev2.7": "pnpm -r --aggregate-output --filter=./playground-vue2.7 dev",
    "dev-node": "pnpm -r --aggregate-output --filter=./playground-node dev",
    "build": "pnpm -r --aggregate-output --filter=./packages/* build",
    "test": "pnpm -r --aggregate-output --filter=./packages/shared test",
    "clear": "rimraf 'packages/*/{dist,node_modules}' && rimraf node_modules",
    "docs:dev": "pnpm -r --aggregate-output --filter=./docs dev",
    "docs:build": "pnpm -r --aggregate-output --filter=./docs build",
    "docs:serve": "pnpm -r --aggregate-output --filter=./docs serve",
    "changeset": "changeset",
    "version-packages": "changeset version"
  },
  "keywords": [],
  "author": "zhensherlock",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@iconify-json/ic": "^1.2.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.3",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.9",
    "@types/markdown-it": "^14.1.2",
    "@typescript-eslint/parser": "^8.7.0",
    "@unocss/postcss": "^0.65.0",
    "autoprefixer": "^10.4.20",
    "codecov": "^3.8.3",
    "concurrently": "^8.2.2",
    "core-js": "^3.38.1",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.22.4",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.13.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.79.3",
    "stylelint": "^16.9.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "terser": "^5.34.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "unocss": "^0.65.0",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-define-options": "^1.5.1",
    "vite": "^5.4.8",
    "vite-plugin-babel": "^1.2.0",
    "vite-plugin-commonjs": "^0.10.3",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "1.4.1",
    "vue-eslint-parser": "^9.4.3"
  },
  "pnpm": {
    "packageExtensions": {
      "vue-template-compiler": {
        "peerDependencies": {
          "vue": "2.6.14"
        }
      }
    }
  }
}
